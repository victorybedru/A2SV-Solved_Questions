class Solution:
    def dividePlayers(self, skill: List[int]) -> int:
        left=0
        right= len(skill)-1
        skill.sort()
        res=0
        target= skill[left]+skill[right]
        while left<right:
            sum1=skill[left]+skill[right]
            if sum1 != target:
                return -1
            res+= skill[left]*skill[right]
            left+=1
            right-=1
            
        return res
